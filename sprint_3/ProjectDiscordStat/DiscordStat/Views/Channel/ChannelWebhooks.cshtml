@model IEnumerable<DiscordStats.ViewModel.WebhookUsageVM>

<div class="mode">
Dark mode:    
    <span class="change">ON</span>
</div>


@if(Model.Count() != 0)
{
	<h1 style="text-align:center">Click on webhook to send message.</h1>
	@foreach(var w in Model)
	{
		<ul>
			<li> <h3><a href="@Url.Action("WebhookMessage", "Channel", new {Name = w.name, Id = w.Id, Token = w.Token, guild_id = w.guild_id})"> @w.name</a></h3>  
				<input type="button" value="Delete Webhook" onclick="location.href='@Url.Action("DeleteWebhook", "Channel", new {Id = w.Id, channel_id= w.channel_id})'"/>
		</ul>

	}
}
else
{

	<h3><a href="@Url.Action("WebhookForm", "Channel", new {channelId = ViewBag.channel_id})"> Create Webhook</a></h3>

}

@section Scripts {

     <script type="text/javascript" src="~/js/site.js"></script>
}